{
  "expo": {
    "name": "Battlefield Stats",
    "slug": "battlefield-stats",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/presentation/BF5_Icon_1024.png",
    "scheme": "battlefieldstats",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 7,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/presentation/BF5_Icon_1024.png",
        "backgroundColor": "#2c3e50"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.elitewise.battlefieldstats",
      "permissions": [
        "INTERNET"
      ],
      "blockedPermissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.SYSTEM_ALERT_WINDOW",
        "android.permission.VIBRATE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/presentation/BF5_Icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/presentation/BF5_Icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0e0e0e"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "191eb208-9d17-40f6-bf91-13e96fd8eb16"
      }
    }
  }
}
